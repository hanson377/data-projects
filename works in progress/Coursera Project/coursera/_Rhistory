alc <- subset(brfss2013, is.na(avedrnk2) == FALSE)
shares <- alc %>%
group_by(avedrnk2) %>%
summarise(
values = n())
shares <- shares %>%
group_by()%>%
mutate(
sum_values=sum(values),
shares = values/sum(values))
shares <- shares %>%
group_by()%>%
mutate(
cum_share = cumsum(shares)
ggplot(alc, aes(avedrnk2)) + stat_ecdf()
